{
        "coc.preferences.formatOnSaveFiletypes": ["go"],
        "cSpellExt.enableDictionaries": ["german"],
        "cSpell.language": "en,de",
        "cSpell.ignoreRegExpList": ["/Plug '.*'/"],
        "languageserver": {
                "terraform": {
                        "command": "terraform-ls",
                        "args": ["serve"],
                        "filetypes": ["terraform", "tf"],
                        "initializationOptions": {},
                        "settings": {}
                },
                "golang": {
                        "command": "~/.config/coc/extensions/coc-go-data/bin/gopls",
                        "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
                        "filetypes": ["go"]
                }
        },
        "yaml.schemas": {}
}
